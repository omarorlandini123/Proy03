@model IEnumerable<Entidades.Material>
@if (Session["usuario"] != null)
{
@if (Model != null && Model.Count() > 0)
{

    foreach (var item in Model)
    {
        <a role="button" class="list-group-item" onclick="setServicio('@(item.codProducto)');">
            <span class="badge">S/ @(item.precioUnit)</span>
            <h4 class="list-group-item-heading">@(item.codProducto)</h4>
            <p class="list-group-item-text">Servicio: @(item.desc)</p>

        </a>


    }

}
else
{
    <a class="list-group-item">
        <p class="list-group-item-text"> No se encuentra el material</p>
    </a>
}
}
else
{
    <div class="modal-content">
        <div class="modal-body">
            <div class="form-group">
                <center>
                    <h3>Su sesión ha expirado, ingrese nuevamente.</h3>
                </center>
            </div>
        </div>
        <div class="modal-footer">
            <div class="col-sm-6 contenido-derecha">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
            </div>

        </div>
    </div>

}