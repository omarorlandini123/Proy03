@model int
@if (Session["usuario"] != null)
{

    <div class="modal-content">
        <div class="modal-body">
            <div class="form-group">
                <center>
                    @switch (Model)
                {
                    case 1:
                            <h3>Se envía la versión correctamente</h3>
                            break;
                        case 2:
                            <h3>La versión, ya fue enviada para aprobación.</h3>
                            break;
                        case 3:
                            <h3>Existen observaciones no resueltas, no se puede aprobar.</h3>
                            break;
                        case 4:
                            <h3>Ud. no tiene permiso para aprobar este detalle.</h3>
                            break;
                        case 5:
                            <h3>Debe registrarse como aprobador de esta versión.</h3>
                            break;
                        case 6:
                            <h3>Debe registrar al menos un detalle</h3>
                            break;

                        case 7:
                            <h3>Todos los detalles deben estar aprobados</h3>
                            break;
                    }
                </center>
            </div>
        </div>
        <div class="modal-footer">
            <div class="col-sm-6 contenido-derecha">
                <button type="button" class="btn btn-default" data-dismiss="modal">Aceptar</button>
            </div>
        </div>
    </div>
}
else
{
    <div class="modal-content">
        <div class="modal-body">
            <div class="form-group">
                <center>
                    <h3>Su sesión ha expirado, ingrese nuevamente.</h3>
                </center>
            </div>
        </div>
        <div class="modal-footer">
            <div class="col-sm-6 contenido-derecha">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
            </div>
        </div>
    </div>

}