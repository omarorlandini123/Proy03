@using Entidades;
@model IEnumerable<Sede>
<div class="row">
    <div class="col-sm-12">
        @Html.Action("BannerConfiguracion", "Login")
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        <h3>SubClases para el gasto de capital</h3>
        <div class="panel panel-primary">

            <div class="panel-heading">
                Aplica a:
            </div>
            <div class="panel-body">
                <div class="row">
                    <label class="col-sm-2" for="" style="text-align:right;">Sede </label>
                    <input type="hidden" id="idSede" value="0" />
                    <div class="col-sm-6">
                        <input class="form-control " id="desSede" value="" type="text" name="desSede" disabled="disabled" placeholder="Buscar Sede">
                    </div>
                    <button type="button" class="col-sm-1 col-sm-offset-1 btn btn-default " onclick="BuscarSedes()">Buscar</button>
                </div>
                <br />
                <div class="row">
                    <label class="col-sm-2" for="" style="text-align:right;">Presupuestos </label>
                    <input type="hidden" id="idPresup" value="0" />
                    <div class="col-sm-6">
                        <input class="form-control " id="desPresup" value="" type="text" name="desPresup" disabled="disabled" placeholder="Buscar Presupuesto">
                    </div>
                    <button type="button" class="col-sm-1 col-sm-offset-1 btn btn-default " onclick="BuscarPresupuestos()">Buscar</button>
                </div>

            </div>
            <div class="panel-footer">
                <div class="row">
                    <div class="col-sm-12" style="text-align:right;">
                        <button type="button" class="btn btn-default" onclick="BuscarClases()">Buscar SubClases</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row" id="arbolClases">



</div>


