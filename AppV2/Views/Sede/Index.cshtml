@model IEnumerable<AppV2.Models.ViewModelSede>

<p>
    @Html.ActionLink("Nueva Sede", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.desSede)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.desSede)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.codSede })
            @Html.ActionLink("Eliminar", "Delete", new { id=item.codSede })
        </td>
    </tr>
}

</table>
